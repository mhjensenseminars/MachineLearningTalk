@Article{Shi2014,
author={Shi, Xuan
and Wei, LianFu
and Oh, Choo Hiap},
title={Quantum computation with surface-state electrons by rapid population passages},
journal={Science China Physics, Mechanics {\&} Astronomy},
year={2014},
month={Sep},
day={01},
volume={57},
number={9},
pages={1718-1724},
abstract={Quantum computation requires coherently controlling the evolutions of qubits. Usually, these manipulations are implemented by precisely designing the durations (such as the $\pi$-pulses) of the Rabi oscillations and tunable interbit coupling. Relaxing this requirement, herein we show that the desired population transfers between the logic states can be deterministically realized (and thus quantum computation could be implemented) both adiabatically and non-adiabatically, by performing the duration-insensitive quantum manipulations. Our proposal is specifically demonstrated with the surface-state of electrons floating on the liquid helium, but could also be applied to the other artificially controllable systems for quantum computing.},
issn={1869-1927},
doi={10.1007/s11433-014-5547-5},
url={https://doi.org/10.1007/s11433-014-5547-5}
}

@article{Dahm2003,
title = {Quantum computing with electrons on helium},
journal = {Physica E: Low-dimensional Systems and Nanostructures},
volume = {18},
number = {1},
pages = {169-172},
year = {2003},
note = {23rd International Conference on Low Temperature Physics (LT23)},
issn = {1386-9477},
doi = {https://doi.org/10.1016/S1386-9477(02)01075-5},
url = {https://www.sciencedirect.com/science/article/pii/S1386947702010755},
author = {Arnold J. Dahm and J.A. Heilman and I. Karakurt and T.J. Peshek},
keywords = {Quantum computing, Decoherence, Electrons on helium},
abstract = {A concept for a quantum computer based on electrons localized above a helium film as quantum logic elements (qubits) is described. Each qubit is made of combinations of the ground and first excited state of an electron trapped in the image potential well at the surface. An array of electrodes beneath the surface restrict the lateral motion of electrons. Proposed mechanisms for one and two qubit logic operations and a simultaneous readout of the final state of all qubits are discussed. The principle decay channel for the excited state is via the emission of one phonon into the bulk liquid. Dephasing is estimated to be dominated by Johnson noise in the control electrodes. These times are on the order of 100μs and allow about 104 operations.}
}

